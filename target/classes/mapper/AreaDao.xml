<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<mapper namespace="com.project1.o2o.dao.AreaDao">
		<select id="queryArea" resultType= "com.project1.o2o.entity.Area">
			Select area_id,area_name,priority,create_time,last_edited
			From tb_area
			Order By priority DESC
		</select>
	</mapper>